
<h2>Order Detail</h2>
<hr style="border: 4px solid red" />
<table class="table">
    <tr>
        <th>
            Product Name:
        </th>
        <th>
            Product Quantity:
        </th>
        <th>
            Product Unit Price:
        </th>



    </tr>
    @foreach (var item in Model)
    {
        <tr>

            <td>
                @item.Product.ProductName
            </td>
            <td>
                @item.Quantity               
            </td>
            <td>
                @item.UnitPrice
            </td>
            


        </tr>

    }
</table>